# Configuration de la base de données
# Remplacez 'user', 'password' et 'dbname' par vos informations PostgreSQL locales
DATABASE_URL=postgres://postgres:password@localhost:5432/telegram_bot_db

# Secret pour la session (utilisez une chaîne longue et aléatoire)
SESSION_SECRET=votre_secret_tres_long_et_aleatoire_ici

# Optionnel : Port de l'application (par défaut 5000)
PORT=5000
